<div class="header" style="padding-top: 80px">
  <div class="container">
    <h3 class="text-center"> Republica de Panama</h3>
    <h4 class="text-center"> Municipio de San Miguelito </h4>
  </div>
</div>
<div class="container">
  <p> <%= link_to "Ver nomina",nominas_path%> </p>

</div>
<div class="container" style="padding-top: 80px">
  <table class="table">
    <thead>
      <tr>
        <th>Acreedores</th>
        <th>1ra</th>
        <th>2da</th>
        <th>Total<th>
        </tr>
      </thead>
      <tbody>
        <% @reportes.each do |reporte| %>
        <tr>
          <td><%= reporte.acreedor %></td>
          <td><%= reporte.pago%></td>
          <td><%= reporte.pago1%></td>
          <td><%= reporte.total%></td>
        </tr>
        <% end %>
      </tbody>
    </table>
    <p> <%= link_to "Ver pdf",reportes_path(format: "pdf")%> </p>
  </div>

